<addon id="repository.hometheater" version="1.0.0" name="My Repository" provider-name="Your Name">
    <extension point="xbmc.addon.repository" name="My Repo">
        <dir>
            <info compressed="false">https://alham0046.github.io/hometheater-kodi-repo/addons.xml</info>
            <checksum>https://alham0046.github.io/hometheater-kodi-repo/addons.xml.md5</checksum>
            <datadir zip="true">https://alham0046.github.io/hometheater-kodi-repo/addons/</datadir>
        </dir>
        <extension point="xbmc.addon.metadata">
            <summary lang="en">My Kodi Addons</summary>
            <description lang="en">A collection of my custom Kodi addons.</description>
            <platform>all</platform>
        </extension>
    </extension>
</addon>

<!-- <?xml version="1.0" encoding="UTF-8" standalone="yes"?> -->
<addon id="script.module.pymongo" name="pymongo" version="4.8.0" provider-name="YourName">
